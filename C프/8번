// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int list[100] =  {5, 8, 7, 0, 0, 2, 1, 9, 2, 8, 0, 2, 2, 4, 7, 5, 4, 9, 7, 7, 0, 0, 8, 6, 9, 9, 7, 4, 2, 7, 4, 7, 8, 1,

   9, 0, 5, 0, 0, 9, 1, 2, 4, 3, 6, 1, 8, 0, 0, 7, 0, 2, 8, 8, 8, 9, 7, 8, 3, 9, 7, 9, 8, 5, 2, 7, 8,

   7, 0, 0, 6, 1, 2, 2, 4, 8, 3, 4, 1, 5, 1, 1, 7, 1, 1, 8, 0, 8, 6, 5, 9, 5, 4, 7, 1, 8, 6, 1, 5, 6  };
  
    
    int save[101]={0};
    
    int count=0;
    int i=0;
    
    
    //==은 비교 연산자이고 =대입 연산자 
    for(int i=0; i<100; i++){
        if(list[i]>=0&&list[i]<=9){
            save[list[i]]++;
            if(save[list[i]]>count&&save[list[i]]==count){
                count=save[list[i]];
            }
        }
    }

    for(i=0; i<101; i++){
        if(save[i]==count){
            printf("같은 값: %d ", i);
        }
    }printf("연속되는 수의 개수: %d\n", count);

    return 0;
}
